[application]

config/name="Fire Emblem"
config/description="Fire Emblem Rebuilt with Godot!"
run/main_scene="res://Scenes/Intro Screen/Intro Screen.tscn"
config/icon="res://FE Icon.jpg"
config/auto_accept_quit=50
config/allowed_rebind=false

[autoload]

Calculators="*res://Engine/Systems/Calculators.gd"
BattlefieldInfo="*res://Engine/Systems/GameSystem.tscn"
Combat_Calculator="*res://Scenes/Combat/Combat_Calculator.gd"
SceneTransition="*res://Scenes/Transition/Scene Transition.tscn"
WorldMapScreen="*res://Scenes/World Map/World Map Screen.tscn"
StatusScreen="*res://Scenes/GUI/Status Screen/Status Screen.tscn"
GroupNames="*res://Engine/Systems/Group Names.gd"
Level="*res://Scenes/Battlefield/Level.tscn"
Convoy="*res://Scenes/Convoy/Convoy Canvas Layer.tscn"

[debug]

settings/fps/force_fps=60
gdscript/warnings/enable=false

[display]

window/size/width=240
window/size/height=160
window/size/test_width=1200
window/size/test_height=800
window/stretch/mode="2d"

[editor_plugins]

enabled=PoolStringArray( "vnen.tiled_importer" )

[importer_defaults]

texture={
"compress/bptc_ldr": 0,
"compress/hdr_mode": 0,
"compress/lossy_quality": 0.7,
"compress/mode": 0,
"compress/normal_map": 0,
"detect_3d": false,
"flags/anisotropic": false,
"flags/filter": false,
"flags/mipmaps": false,
"flags/repeat": 0,
"flags/srgb": 2,
"process/HDR_as_SRGB": false,
"process/fix_alpha_border": true,
"process/invert_color": false,
"process/premult_alpha": false,
"size_limit": 0,
"stream": false,
"svg/scale": 1.0
}
vnen.tiled_importer={
"add_background": true,
"collision_layer": 1,
"custom_properties": true,
"embed_internal_images": false,
"image_flags": 7,
"post_import_script": "",
"save_tiled_properties": true,
"tile_metadata": true,
"uv_clip": true
}
wav={
"compress/mode": 0,
"edit/loop": true,
"edit/normalize": true,
"edit/trim": true,
"force/8_bit": false,
"force/max_rate": false,
"force/max_rate_hz": 44100,
"force/mono": false
}

